   "source": [
    "import os\n",
    "key=os.urandom(32).hex()\n",
    "with open(\".env\") as f:\n",
    "    name=f.read()\n",
    "    if \"jwtSecret\" not in name:\n",
    "        with open(\".env\",\"a\") as f:\n",
    "            f.write(\"jwtSecret=\"+key)\n",
    "            print(key[::5])\n",
    "    else:\n",
    "        print(\"jwtSecret already exists\")\n"
   ]
  }
